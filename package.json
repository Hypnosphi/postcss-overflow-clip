{
  "name": "postcss-overflow-clip",
  "version": "0.4.1",
  "description": "PostCSS plugin that adds `overvlow: clip` whenever `overflow: hidden` is used and vice versa",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "overflow"
  ],
  "scripts": {
    "test": "vitest run",
    "watch": "vitest watch",
    "lint-fix": "eslint *.js --fix"
  },
  "author": "Carlos Jeurissen",
  "license": "MIT",
  "repository": "Hypnosphi/postcss-overflow-clip",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "postcss-value-parser": "^4.2.0"
  },
  "peerDependencies": {
    "postcss": ">=8.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-ccb": "^0.16.4",
    "postcss": "^8.4.35",
    "vitest": "^1.2.2"
  },
  "eslintConfig": {
    "extends": "ccb",
    "env": {
      "es2022": true,
      "node": true
    }
  }
}
